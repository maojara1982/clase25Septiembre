<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:orientation="vertical"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

       <Button
           android:text="@string/irgoogle"
           android:id="@+id/google"
           android:layout_width="match_parent"
           android:layout_height="wrap_content">

       </Button>

        <Button
            android:text="@string/iralarma"
            android:id="@+id/alarma"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </Button>

        <Button
            android:text="@string/irllamada"
            android:id="@+id/llamada"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </Button>

        <Button
            android:text="@string/irtempo"
            android:id="@+id/temporizador"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </Button>

        <Button
            android:text="@string/iracamara"
            android:id="@+id/camara"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </Button>






    </LinearLayout>

</LinearLayout>